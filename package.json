{
  "name": "coachplan",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "prisma generate && next dev --port 3001",
    "build": "prisma generate && next build",
    "postinstall": "prisma generate",
    "start": "next start",
    "lint": "eslint",
    "audit:auth": "node scripts/save-audit-auth-state.mjs",
    "audit:auth:onboarding": "node scripts/save-audit-auth-state.mjs --profile onboarding-athlete --start-path /sign-up",
    "audit:screens": "node scripts/capture-audit.mjs",
    "audit:screens:onboarding": "node scripts/capture-audit.mjs --profile onboarding-athlete --out-dir artifacts/audit-shots-onboarding",
    "audit:screens:manual": "node scripts/capture-audit-manual.mjs",
    "audit:screens:manual:onboarding": "node scripts/capture-audit-manual.mjs --profile onboarding-athlete --out-dir artifacts/audit-shots-onboarding-manual",
    "test:parser-i18n": "node scripts/test-plan-parser-i18n.mjs",
    "make-admin": "node scripts/promote-admin.js",
    "backfill-plan-dates": "node scripts/backfill-plan-dates.js"
  },
  "dependencies": {
    "@clerk/nextjs": "^6.37.2",
    "@prisma/client": "^5.22.0",
    "next": "16.1.6",
    "pdf-parse": "^1.1.1",
    "pdfjs-dist": "^4.10.38",
    "react": "19.2.3",
    "react-dom": "19.2.3"
  },
  "devDependencies": {
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "playwright": "^1.58.2",
    "prisma": "^5.22.0",
    "typescript": "^5"
  }
}
